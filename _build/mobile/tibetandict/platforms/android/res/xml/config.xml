<?xml version='1.0' encoding='utf-8'?>
<widget id="de.christian_steinert.tibetandict" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0"  android-versionCode="199">
    <preference name="loglevel" value="DEBUG" />
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="SQLitePlugin">
        <param name="android-package" value="io.sqlc.SQLitePlugin" />
    </feature>
    <name>TibetanDictionary</name>
    <description>A Tibetan Dictionary that supports Wylie input, uses Tibetan script and comes with various dictionaries.</description>
    <author email="" href="http://www.christian-steinert.de">Christian Steinert</author>
    <content src="index.html" />
    <access origin="*" />
</widget>
