               <value>Gu-Ltn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Gujarti transcribed in Roman script</a:documentation>
               <value>Hi-Ltn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Hindi transcribed in Roman script</a:documentation>
               <value>Ja-Ltn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Japanese transcribed in Roman script</a:documentation>
               <value>Kn-Ltn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Kannada transcribed in Roman script</a:documentation>
               <value>Mr-Ltn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Marathi transcribed in Roman script</a:documentation>
               <value>Pi-Ltn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Pali transcribed in Roman script</a:documentation>
               <value>Pra-Ltn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Prakrit transcribed in Roman script</a:documentation>
               <value>Sa-Ltn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Sanskrit transcribed in Roman script (Note: the term will be italicized. do not tag this for terms that are left unitalicized in 84000 texts like sutra, nagas, dharma, Sanskrit names, etc.)</a:documentation>
               <value>Ta-Ltn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Tamil transcribed in Roman script</a:documentation>
               <value>Th-Ltn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Thai transcribed in Roman script</a:documentation>
               <value>Zh-Ltn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Chinese transcribed in Roman script</a:documentation>
               <value>bo</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Tibetan in Tibetan script (Use Bo-Ltn if in Roman script or Wylie) </a:documentation>
               <value>de</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">German (Deutsch)</a:documentation>
               <value>el</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Greek</a:documentation>
               <value>en</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">English</a:documentation>
               <value>fr</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">French</a:documentation>
               <value>it</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Italian</a:documentation>
               <value>la</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Latin</a:documentation>
               <value>zh</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Chinese in Chinese script (Use Zh-Ltn if in Roman script)</a:documentation>
               <value>middle_indic-Ltn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Middle Indic transcribed in Roman script.</a:documentation>
            </choice>
         </attribute>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">classifies the title according to the TBRC typology.classifies the title according to the 84000 typology.</a:documentation>
               <choice>
                  <value>mainTitle</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <value>longTitle</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <value>otherTitle</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="publisher">
      <element name="publisher">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides the name of the organization responsible for the publication or distribution of a bibliographic item. [3.11.2.4. Imprint, Size of a Document, and Reprint Information 2.2.4. Publication, Distribution, Licensing, etc.]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.canonical.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="biblScope">
      <element name="biblScope">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(scope of bibliographic reference) defines the scope of a bibliographic reference, for example as a list of page numbers, or a named subdivision of a larger work. [3.11.2.5. Scopes and Ranges in Bibliographic Citations]</a:documentation>
         <ref name="macro.phraseSeq"/>
      </element>
   </define>
   <define name="bibl">
      <element name="bibl">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(bibliographic citation) contains a loosely-structured bibliographic citation of which the sub-components may or may not be explicitly tagged. [3.11.1. Methods of Encoding Bibliographic References and Lists of References 2.2.7. The Source Description 15.3.2. Declarable Elements]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.gLike"/>
               <ref name="model.highlighted"/>
               <ref name="model.pPart.data"/>
               <ref name="model.pPart.edit"/>
               <ref name="model.segLike"/>
               <ref name="model.ptrLike"/>
               <ref name="model.biblPart"/>
               <ref name="model.global"/>
            </choice>
         </zeroOrMore>
         <ref name="att.global.attribute.xmlid"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.</a:documentation>
               <data type="token">
                  <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
               </data>
            </attribute>
         </optional>
         <optional>
            <attribute name="key">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="l">
      <element name="l">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(verse line) contains a single, possibly incomplete, line of verse. [3.12.1. Core Tags for Verse 3.12. Passages of Verse or Drama 7.2.5. Speech Contents]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.gLike"/>
               <ref name="model.phrase"/>
               <ref name="model.inter"/>
               <ref name="model.global"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="NewODD-l-abstractModel-structure-l-constraint-report-5">
            <rule context="tei:l">
               <report xmlns:rng="http://relaxng.org/ns/structure/1.0" test="ancestor::tei:l[not(.//tei:note//tei:l[. = current()])]">
        Abstract model violation: Lines may not contain lines or lg elements.
      </report>
            </rule>
         </pattern>
      </element>
   </define>
   <define name="lg">
      <element name="lg">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(line group) contains one or more verse lines functioning as a formal unit, e.g. a stanza, refrain, verse paragraph, etc. [3.12.1. Core Tags for Verse 3.12. Passages of Verse or Drama 7.2.5. Speech Contents]</a:documentation>
         <oneOrMore>
            <choice>
               <ref name="l"/>
               <ref name="milestone"/>
               <ref name="seg"/>
            </choice>
         </oneOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="NewODD-lg-atleast1oflggapl-constraint-assert-3">
            <rule context="tei:lg">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:rng="http://relaxng.org/ns/structure/1.0" test="count(descendant::tei:lg|descendant::tei:l|descendant::tei:gap) &gt; 0">An lg element
        must contain at least one child l, lg or gap element.</sch:assert>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="NewODD-lg-abstractModel-structure-l-constraint-report-6">
            <rule context="tei:lg">
               <report xmlns:rng="http://relaxng.org/ns/structure/1.0" test="ancestor::tei:l[not(.//tei:note//tei:lg[. = current()])]">
        Abstract model violation: Lines may not contain line groups.
      </report>
            </rule>
         </pattern>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Used to denote specific types of verses such as "sdom" or "bar_sdom" indexes and so forth.</a:documentation>
               <choice>
                  <value>sdom</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <value>bar_sdom</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <value>spyi_sdom</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </choice>
            </attribute>
         </optional>
         <attribute name="tid">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Numbers paragraph and line groups in a particular text.</a:documentation>
            <text/>
         </attribute>
         <optional>
            <attribute name="rend">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <choice>
                  <value>mantra</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">This lg element contains a mantra. The mantra has been formally and clearly declared and so will set apart with indentation and vertical spacing in the 84000 web display. Not all lg elements that contain mantras need to be rendered in this way, but generally we do so, when they are formally and clearly declared in the text.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="teiHeader">
      <element name="teiHeader">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(TEI header) supplies descriptive and declarative metadata associated with a digital resource or set of resources. [2.1.1. The TEI Header and Its Components 15.1. Varieties of Composite Text]</a:documentation>
         <element name="fileDesc">
            <element name="titleStmt">
               <optional>
                  <attribute name="sponsored">
                     <choice>
                        <value>full</value>
                        <value>part</value>
                        <value/>
                     </choice>
                  </attribute>
               </optional>
               <zeroOrMore>
                  <ref name="title"/>
               </zeroOrMore>
               <zeroOrMore>
                  <choice>
                     <optional>
                        <element name="author">
                           <attribute name="role">
                              <value>translatorMain</value>
                           </attribute>
                           <optional>
                              <attribute name="ref"/>
                           </optional>
                           <zeroOrMore>
                              <choice>
                                 <text/>
                                 <ref name="lb"/>
                              </choice>
                           </zeroOrMore>
                        </element>
                     </optional>
                     <zeroOrMore>
                        <element name="author">
                           <attribute name="role">
                              <choice>
                                 <value>translatorEng</value>
                                 <value>translatorTib</value>
                                 <value>preface</value>
                              </choice>
                           </attribute>
                           <optional>
                              <attribute name="ref"/>
                           </optional>
                           <zeroOrMore>
                              <text/>
                           </zeroOrMore>
                        </element>
                     </zeroOrMore>
                     <zeroOrMore>
                        <ref name="editor"/>
                     </zeroOrMore>
                     <zeroOrMore>
                        <ref name="consultant"/>
                     </zeroOrMore>
                     <zeroOrMore>
                        <ref name="sponsor"/>
                     </zeroOrMore>
                  </choice>
               </zeroOrMore>
            </element>
            <zeroOrMore>
               <ref name="editionStmt"/>
            </zeroOrMore>
            <choice>
               <ref name="publicationStmt"/>
               <element name="publicationStmt">
                  <attribute name="status"/>
                  <element name="idno">
                     <attribute name="id" ns="http://www.w3.org/XML/1998/namespace">
                        <data type="ID"/>
                     </attribute>
                  </element>
                  <zeroOrMore>
                     <choice>
                        <element name="note">
                           <text/>
                        </element>
                        <element name="date">
                           <rng:empty xmlns:rng="http://relaxng.org/ns/structure/1.0"/>
                        </element>
                     </choice>
                  </zeroOrMore>
               </element>
            </choice>
            <element name="sourceDesc">
               <oneOrMore>
                  <element name="bibl">
                     <attribute name="key">
                        <data type="string"/>
                     </attribute>
                     <attribute name="type">
                        <choice>
                           <value>text</value>
                           <value>chapter</value>
                        </choice>
                     </attribute>
                     <element name="ref">
                        <data type="string"/>
                     </element>
                     <text/>
                     <ref name="biblScope"/>
                     <optional>
                        <element name="series">
                           <data type="string"/>
                        </element>
                     </optional>
                     <optional>
                        <element name="citedRange">
                           <data type="string"/>
                        </element>
                     </optional>
                     <zeroOrMore>
                        <element name="author">
                           <attribute name="role">
                              <value>translatorTib</value>
                           </attribute>
                           <text/>
                        </element>
                     </zeroOrMore>
                     <element name="location">
                        <attribute name="count-pages">
                           <data type="integer"/>
                        </attribute>
                        <element name="start">
                           <attribute name="volume">
                              <data type="integer"/>
                           </attribute>
                           <attribute name="page">
                              <data type="integer"/>
                           </attribute>
                        </element>
                        <element name="end">
                           <attribute name="volume">
                              <data type="integer"/>
                           </attribute>
                           <attribute name="page">
                              <data type="integer"/>
                           </attribute>
                        </element>
                     </element>
                     <element name="idno">
                        <attribute name="parent-id">
                           <text/>
                        </attribute>
                        <optional>
                           <attribute name="work">
                              <text/>
                           </attribute>
                        </optional>
                     </element>
                     <optional>
                        <element name="idno">
                           <attribute name="source-id">
                              <text/>
                           </attribute>
                           <attribute name="work">
                              <text/>
                           </attribute>
                        </element>
                     </optional>
                  </element>
               </oneOrMore>
            </element>
            <optional>
               <element name="notesStmt">
                  <zeroOrMore>
                     <element name="note">
                        <optional>
                           <attribute name="type"/>
                        </optional>
                        <optional>
                           <attribute name="date-time"/>
                        </optional>
                        <optional>
                           <attribute name="value"/>
                        </optional>
                        <optional>
                           <attribute name="user"/>
                        </optional>
                        <optional>
                           <attribute name="update"/>
                        </optional>
                        <text/>
                     </element>
                  </zeroOrMore>
               </element>
            </optional>
         </element>
      </element>
   </define>
   <define name="titleStmt">
      <element name="titleStmt">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(title statement) groups information about the title of a work and those responsible for its content. [2.2.1. The Title Statement 2.2. The File Description]</a:documentation>
         <group>
            <oneOrMore>
               <ref name="title"/>
            </oneOrMore>
            <zeroOrMore>
               <ref name="model.respLike"/>
            </zeroOrMore>
         </group>
         <optional>
            <attribute name="sponsored">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Identifies in the titleStmt, whether the text has been sponsored in part or full.</a:documentation>
               <choice>
                  <value>full</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <value>part</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <value/>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="editionStmt">
      <element name="editionStmt">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(edition statement) groups information relating to one edition of a text. [2.2.2. The Edition Statement 2.2. The File Description]</a:documentation>
         <choice>
            <oneOrMore>
               <ref name="model.pLike"/>
            </oneOrMore>
            <group>
               <ref name="edition"/>
               <zeroOrMore>
                  <ref name="model.respLike"/>
               </zeroOrMore>
            </group>
         </choice>
      </element>
   </define>
   <define name="edition">
      <element name="edition">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">describes the particularities of one edition of a text. [2.2.2. The Edition Statement]</a:documentation>
         <text/>
         <optional>
            <ref name="date"/>
         </optional>
      </element>
   </define>
   <define name="publicationStmt">
      <element name="publicationStmt">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(publication statement) groups information concerning the publication or distribution of an electronic or other text. [2.2.4. Publication, Distribution, Licensing, etc. 2.2. The File Description]</a:documentation>
         <group>
            <element name="publisher">
               <oneOrMore>
                  <choice>
                     <text/>
                     <optional>
                        <ref name="name"/>
                     </optional>
                     <zeroOrMore>
                        <ref name="lb"/>
                     </zeroOrMore>
                     <zeroOrMore>
                        <ref name="emph"/>
                     </zeroOrMore>
                  </choice>
               </oneOrMore>
            </element>
            <element name="availability">
               <zeroOrMore>
                  <element name="p">
                     <attribute name="type">
                        <value>tantricRestriction</value>
                     </attribute>
                     <zeroOrMore>
                        <choice>
                           <text/>
                           <ref name="emph"/>
                        </choice>
                     </zeroOrMore>
                  </element>
               </zeroOrMore>
               <ref name="licence"/>
            </element>
            <element name="idno">
               <attribute name="id" ns="http://www.w3.org/XML/1998/namespace">
                  <data type="ID"/>
               </attribute>
            </element>
            <zeroOrMore>
               <element name="note">
                  <text/>
               </element>
            </zeroOrMore>
            <element name="date">
               <data type="date"/>
            </element>
         </group>
         <attribute name="status">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Identifies what stage the text is in preparation for publication.</a:documentation>
            <choice>
               <value>1</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Translations completed and published</a:documentation>
               <value>2.a</value>
